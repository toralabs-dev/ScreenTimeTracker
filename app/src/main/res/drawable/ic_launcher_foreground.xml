<?xml version="1.0" encoding="utf-8"?>
<!-- Launcher icon foreground - clock/timer icon representing screen time -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Clock circle outline -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="@color/md_theme_light_primary"
        android:strokeWidth="3"
        android:pathData="M54,54m-24,0a24,24 0,1 1,48 0a24,24 0,1 1,-48 0" />

    <!-- Clock hour hand -->
    <path
        android:fillColor="@color/md_theme_light_primary"
        android:pathData="M54,54L54,40"
        android:strokeWidth="3"
        android:strokeColor="@color/md_theme_light_primary"
        android:strokeLineCap="round" />

    <!-- Clock minute hand -->
    <path
        android:fillColor="@color/md_theme_light_primary"
        android:pathData="M54,54L66,54"
        android:strokeWidth="3"
        android:strokeColor="@color/md_theme_light_primary"
        android:strokeLineCap="round" />

    <!-- Center dot -->
    <path
        android:fillColor="@color/md_theme_light_primary"
        android:pathData="M54,54m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />

</vector>
